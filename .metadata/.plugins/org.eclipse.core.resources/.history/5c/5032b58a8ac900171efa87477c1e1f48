MOTOR_SIGNAL_t* mtr_sg_mock;
Ensure(test_algorithm)
{
	//If centre == too_near

	mtr_sg_mock = (MOTOR_SIGNAL_t*)obstacle_avoidance(25,25,25,25);
	assert_that((mtr_sg_mock->MOTOR_DRIVE_FORWARD), is_equal_to(0));
	assert_that((mtr_sg_mock->MOTOR_DRIVE_NEUTRAL), is_equal_to(0));
	assert_that((mtr_sg_mock->MOTOR_DRIVE_REVERSE), is_equal_to(0));
	assert_that((mtr_sg_mock->MOTOR_DRIVE_SPEED), is_equal_to(0));
	assert_that((mtr_sg_mock->MOTOR_STEER_ANGLE), is_equal_to(0));
	assert_that((mtr_sg_mock->MOTOR_STEER_FULL_LEFT), is_equal_to(0));
	assert_that((mtr_sg_mock->MOTOR_STEER_SLIGHT_LEFT), is_equal_to(0));
	assert_that((mtr_sg_mock->MOTOR_STEER_FULL_RIGHT), is_equal_to(0));
	assert_that((mtr_sg_mock->MOTOR_STEER_SLIGHT_RIGHT), is_equal_to(0));
	assert_that((mtr_sg_mock->MOTOR_STEER_STRAIGHT), is_equal_to(1));

    mtr_sg_mock = {0};

    mtr_sg_mock = (MOTOR_SIGNAL_t*)obstacle_avoidance(25,25,25,26);
    assert_that((mtr_sg_mock->MOTOR_DRIVE_FORWARD), is_equal_to(0));
    assert_that((mtr_sg_mock->MOTOR_DRIVE_NEUTRAL), is_equal_to(0));
    assert_that((mtr_sg_mock->MOTOR_DRIVE_REVERSE), is_equal_to(0));
    assert_that((mtr_sg_mock->MOTOR_DRIVE_SPEED), is_equal_to(0));
    assert_that((mtr_sg_mock->MOTOR_STEER_ANGLE), is_equal_to(0));
    assert_that((mtr_sg_mock->MOTOR_STEER_FULL_LEFT), is_equal_to(0));
    assert_that((mtr_sg_mock->MOTOR_STEER_SLIGHT_LEFT), is_equal_to(0));
    assert_that((mtr_sg_mock->MOTOR_STEER_FULL_RIGHT), is_equal_to(0));
    assert_that((mtr_sg_mock->MOTOR_STEER_SLIGHT_RIGHT), is_equal_to(0));
    assert_that((mtr_sg_mock->MOTOR_STEER_STRAIGHT), is_equal_to(1));

    mtr_sg_mock = {0};

